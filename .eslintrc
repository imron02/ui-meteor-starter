{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "rules": {
    "react/jsx-filename-extension": [
      "off"
    ],
    "no-undef": [
      "error"
    ],
    "react/sort-comp": [
      "off"
    ],
    "react/prefer-stateless-function": [
      "off"
    ],
    "react/prop-types": [
      "error",
      {
        "ignore": [
          "navigation"
        ]
      }
    ],
    "comma-dangle": [
      "error",
      {
        "arrays": "never",
        "objects": "never",
        "imports": "never",
        "exports": "never",
        "functions": "ignore"
      }
    ]
  },
  "globals": {
    "it": false,
    "expect": false,
    "describe": false,
    "navigator": false
  }
}