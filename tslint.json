{
  "extends": [
    "tslint-react-recommended",
    "tslint-config-standard"
  ],
  "rules": {
    "ordered-imports": [
      true
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "member-access": false,
    "member-ordering": [
      false
    ],
    "trailing-comma": [
      true,
      {
        "singleline": "never",
        "multiline": {
          "objects": "never",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "ignore"
        }
      }
    ],
    "no-empty": false,
    "no-submodule-imports": false,
    "no-implicit-dependencies": false,
    "no-constant-condition": false,
    "triple-equals": [
      true,
      "allow-undefined-check"
    ],
    "ter-indent": [
      true,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "no-duplicate-imports": false,
    "jsx-alignment": true,
    "jsx-no-bind": true,
    "jsx-no-lambda": true,
    "max-classes-per-file": false,
    "semicolon": true,
    "no-unused-variable": true,
    "space-before-function-paren": false
  }
}